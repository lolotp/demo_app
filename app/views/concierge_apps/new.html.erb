<%= form_for @concierge_app, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  Country: <%= f.text_field :iso_country_code %>
  Category: <%= f.select(:category, options_for_select([['Games'], ['Utility'], ['Travel'], ['Emergency'], ['Shopping']])) %>
  Link: <%= f.text_field :app_store_link %>
  <%= f.submit "Create" %>
<% end %>
